<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        <h2> こんにちは </h2>
        <p><%= link_to current_user.name, "/users/#{current_user.id}", class: :greeting__link%>さん</p>
      </div>
    <% end %>
    <div class="card__wrapper">
      <%= render partial: 'prototype', collection: @prototypes %>
    </div>
  </div>
</main>
